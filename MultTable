package ch01;
//곱셈표를 출력하는 프로그램
public class MultTable {
	public static void main(String[] args) {
		
		System.out.print
		("   |");
		
		for(int i=1;i<10;i++)
			System.out.printf("%3d",i);
		System.out.println("");
		System.out.println("---+----------------------------");
		for(int i=1;i<=9;i++) {
			System.out.printf("%3d|",i);
			for(int j=1;j<10;j++) {
				System.out.printf("%3d",i*j);
			}
			System.out.println("");
		}
	}
}
